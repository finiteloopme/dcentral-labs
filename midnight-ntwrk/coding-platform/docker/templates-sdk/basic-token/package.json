{
  "name": "midnight-token-dapp",
  "version": "0.1.0",
  "description": "Basic token DApp for Midnight Network with SDK integration",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "node --experimental-specifier-resolution=node src/index.js",
    "start": "node src/index.js",
    "cli": "node --experimental-specifier-resolution=node src/cli.js",
    "test": "node test/token.test.js",
    "compile": "compactc contracts/Token.compact ./src/managed/token",
    "build": "npm run compile",
    "proof-server": "docker compose -f proof-server.yml up -d",
    "proof-server-stop": "docker compose -f proof-server.yml down",
    "proof-server-logs": "docker compose -f proof-server.yml logs -f",
    "proof-server-status": "curl -s http://localhost:6300/health || echo 'Proof server not running'"
  },
  "keywords": [
    "midnight",
    "blockchain",
    "token",
    "dapp",
    "zero-knowledge"
  ],
  "author": "Midnight Developer",
  "license": "MIT",
  "dependencies": {
    "@midnight-ntwrk/compact-runtime": "^0.8.1",
    "@midnight-ntwrk/midnight-js-contracts": "^2.0.2",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "^2.0.2",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "^2.0.2",
    "@midnight-ntwrk/midnight-js-level-private-state-provider": "^2.0.2",
    "@midnight-ntwrk/midnight-js-network-id": "^2.0.2",
    "@midnight-ntwrk/midnight-js-node-zk-config-provider": "^2.0.2",
    "@midnight-ntwrk/midnight-js-types": "^2.0.2",
    "@midnight-ntwrk/midnight-js-utils": "^2.0.2",
    "@midnight-ntwrk/wallet": "^5.0.0",
    "@midnight-ntwrk/wallet-api": "^5.0.0",
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@midnight-ntwrk/compact-compiler": "^0.23.0",
    "nodemon": "^3.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "typescript": "^5.9.3",
    "@types/node": "^24.8.1"
  }
}