{
  "server": {
    "port": 8080,
    "host": "0.0.0.0"
  },
  "proof": {
    "defaultProtocol": "groth16",
    "supportedProtocols": ["groth16", "plonk", "stark"],
    "maxProofSize": 10485760,
    "timeout": 30000
  },
  "circuit": {
    "maxConstraints": 1000000,
    "optimization": true,
    "cacheEnabled": true,
    "cacheTTL": 3600
  },
  "security": {
    "rateLimit": {
      "enabled": true,
      "windowMs": 900000,
      "max": 100
    },
    "cors": {
      "enabled": true,
      "origins": ["*"]
    }
  },
  "logging": {
    "level": "info",
    "format": "json"
  }
}