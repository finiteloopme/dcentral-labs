{
  "name": "@midnight/cli",
  "version": "0.1.0",
  "description": "Midnight development CLI utility",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "midnightctl": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "test:integration": "vitest run --config vitest.integration.config.ts"
  },
  "keywords": [
    "midnight",
    "blockchain",
    "zero-knowledge",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "comments": {
    "sdk": "Midnight SDK 3.x packages built from source. See: make build-sdk",
    "paths": "These file: paths reference /opt/vendor in the container",
    "upgrade": "When SDK 3.x is published to npm, replace file: paths with ^3.0.0 versions",
    "transitive": "Dependencies below the vendor packages are transitive deps required by SDK packages"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",

    "@midnight-ntwrk/ledger-v6": "file:/opt/vendor/ledger/ledger-v6",
