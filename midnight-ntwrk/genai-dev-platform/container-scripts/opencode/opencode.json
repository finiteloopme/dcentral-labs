{
  "$schema": "https://opencode.ai/config.json",
  "model": "google-vertex/gemini-2.5-pro",
  "small_model": "google-vertex/gemini-2.5-flash",
  "disabled_providers": ["anthropic", "google-vertex-anthropic"],
  "autoupdate": false,
  "share": "manual",
  "instructions": [
    "AGENTS.md",
    "README.md",
    "WALLET-HOW-TO.md"
  ],
  "provider": {
    "google-vertex": {
      "models": {
        "gemini-2.5-pro": {},
        "gemini-2.5-flash": {},
        "gemini-2.0-flash": {}
      },
      "options": {
        "project": "{env:GOOGLE_CLOUD_PROJECT}",
        "location": "{env:VERTEX_LOCATION}"
      }
    }
  }
}
